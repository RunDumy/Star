{
  "version": 2,
  "builds": [
    {
      "src": "star-frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/star-frontend/api/proxy.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/star-frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://star-backend.onrender.com"
  }
}
