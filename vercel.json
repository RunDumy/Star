{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "framework": null,
    "rootDirectory": "star-frontend",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://star-backend-l7u1.onrender.com/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "https://star-backend-l7u1.onrender.com"
    }
}