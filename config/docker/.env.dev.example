# Example env file for docker-compose.dev.yml
# Copy to .env.dev and update secrets before running docker-compose

# Supabase/Postgres
POSTGRES_DB=star_db
POSTGRES_PASSWORD=change_me_secure_password

# Supabase web URL and anon key used by backend/frontend
SUPABASE_URL=http://supabase:3000
SUPABASE_ANON_KEY=replace_with_local_anon_key
SUPABASE_KEY=replace_with_local_anon_key

# Frontend
NEXT_PUBLIC_API_URL=http://backend:5000

# Any other env vars for local dev
FLASK_ENV=development
